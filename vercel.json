{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"  
      },
      {
        "source": "/(.*)",
        "destination": "/public/home/$1"  
      },
      {
        "source": "/admin/(.*)",
        "destination": "/dist/$1"  
      }
    ]
  }